@model ContactModel
<div class="row">
	<div class="col-sm-2">
		<div class="companyinfo">
			<p><img src="~/media/logo/@Model.LogoImg" width="200px" height="120px" /></p>
			<p>@Html.Raw(Model.Description)</p>
		</div>
	</div>
	<div class="col-sm-7">
		<div class="col-sm-5">
			@Html.Raw(Model.Map)
		</div>

		

		
	</div>
	<div class="col-sm-3">
		<div class="address">
			<h5>Name : @Model.Name</h5>
			<h5>Email : @Model.Email</h5>
			<h5>Phone : @Model.Phone</h5>

		</div>
	</div>
</div>